    "#Scores by school spending\n",
    "\n",
    "spending_bins = [0, 585, 615, 645, 675]\n",
    "group_names = [\"<$585\", \"$585-615\", \"$615-645\", \"$645-675\"]\n",
    "\n",
    "df = BestSchools.loc[:,[\"Average Math Score\",\n",
    "                  \"Average Reading Score\",\"% Passing Math\",\n",
    "                  \"% Passing Reading\",\"% Overall Passing Rate\",\"Per Student Budget\"]]\n",
    "df.loc[:,\"Spending Ranges (Per Student)\"] = pd.cut(df.loc[:,\"Per Student Budget\"], spending_bins, labels=group_names)\n",
    "SchoolSpendingBins = df.groupby(\"Spending Ranges (Per Student)\")\n",
    "SchoolSpendingBins.mean()\n",
    "\n"
   ]
