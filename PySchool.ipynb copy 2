    "df = complete_data_set[[\"school_name\",\"grade\",\"math_score\"]]\n",
    "\n",
    "ninth=df.loc[df[\"grade\"] == \"9th\"]\n",
    "\n",
    "tenth=df.loc[df[\"grade\"] == \"10th\"]\n",
    "eleventh=df.loc[df[\"grade\"] == \"11th\"]\n",
    "twelth=df.loc[df[\"grade\"] == \"12th\"]\n",
    "\n",
    "ninthmathscores = (ninth.groupby(\"school_name\").math_score.sum())/(ninth.groupby(\"school_name\").math_score.count())\n",
    "tenthmathscores = (tenth.groupby(\"school_name\").math_score.sum())/(tenth.groupby(\"school_name\").math_score.count())\n",
    "eleventhmathscores = (eleventh.groupby(\"school_name\").math_score.sum())/(eleventh.groupby(\"school_name\").math_score.count())\n",
    "twelthmathscores = (twelth.groupby(\"school_name\").math_score.sum())/(twelth.groupby(\"school_name\").math_score.count())\n"
   ]
