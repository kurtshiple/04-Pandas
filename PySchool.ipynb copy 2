    "student_data = pd.read_csv(student_data_to_load)\n",
    "\n",
    "#Combining the data into one set\n",
    "complete_data_set = pd.merge(student_data, school_data, how=\"left\", on=[\"school_name\"])\n",
    "complete_data_set.head(10)"
   ]
