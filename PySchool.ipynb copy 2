    "#This merges our columns together and renames those columns to produce a school summary\n",
    "columns = [schooltype, totalstudentsbyschool, totalbudgetbyschool, budgetperstudent,mathscoreaveragebyschool, \n",
    "           readingscoreaveragebyschool, mathpassingratebyschool,\n",
    "            readingpassingratebyschool, overallpassingratebyschool]\n",
    "\n",
    "SchoolSummary = pd.concat(columns, axis=1)\n",
    "\n",
    "SchoolSummary.index.names = [' ']\n",
    "\n",
    "SchoolSummary.columns = ['School Type',\n",
    "      'Total Students',\n",
